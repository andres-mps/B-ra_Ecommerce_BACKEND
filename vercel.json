{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "/public//", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/css/(.)",
      "headers": {
        "Access-Control-Allow-Origin": ""
      },
      "dest": "/public/css/$1"
    },
    {
      "src": "/(.)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/server.js"
    }
  ]
}
